{
  "name": "savingsweb",
  "version": "1.0.0",
  "description": "Savings web app",
  "private": true,
  "scripts": {
    "clean": "rimraf dist",
    "start:babel": "babel src/index.js  --out-file=public/main.js --watch",
    "start:babel:sm": "babel src/index.js  --out-file=public/main.js --source-maps --watch",
    "start": "MODE=dev webpack-dev-server --config config/webpack.config.js",
    "build": "npm run clean && webpack --config config/webpack.config.js"
  },
  "author": "Deepak",
  "license": "ISC",
  "dependencies": {
    "react": "^16.13.0",
    "react-dom": "^16.13.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-arrow-functions": "^7.8.3",
    "babel-loader": "^8.0.6",
    "file-loader": "^5.1.0",
    "html-webpack-plugin": "^3.2.0",
    "live-server": "^1.2.1",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
}
